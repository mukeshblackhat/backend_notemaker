{
    "version":2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"

        }
    ],
    "routes":[
        {
            "src": "/al/(.*)", 
   
            "dest": "/index.js" 
          
        },
        {
            "src": "/a/(.*)", 
            "dest": "/index.js" 
        },
        {
            "src": "/note/(.*)", 
            "dest": "/index.js" 
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}